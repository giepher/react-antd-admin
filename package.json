  "description": "React Admin Boilerplate, with React + Ant Design",
  "author": "jiangxy <foolbeargm@gmail.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=5.0 <7",
    "npm": ">=3.3 <4"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jiangxy/react-antd-admin.git"
  },
  "keywords": [
    "ant",
    "react",
    "admin",
    "frontend"
  ],
  "devDependencies": {
    "antd": "4.0.3",
    "compression-webpack-plugin": "3.1.0",
    "cross-env": "7.0.2",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.x",
    "eslint-plugin-react": "7.x",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "5.1.0",
    "html-webpack-plugin": "3.2.0",
    "less": "3.11.1",
    "less-loader": "5.0.0",
    "lesshint": "6.3.7",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-hot-loader": "4.12.20",
    "react-redux": "7.2.0",
    "react-router": "5.1.2",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "rimraf": "3.0.2",
    "strip-loader": "0.1.2",
    "style-loader": "1.1.3",
    "stylelint": "13.2.1",
    "stylelint-config-standard": "20.0.0",
    "url-loader": "3.0.0",
    "webpack": "4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "dependencies": {
    "superagent": "5.2.2"
  },
  "scripts": {
    "build": "webpack --progress --colors --display-error-details",
    "prod": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.prod.js -p --display-error-details",
    "clean": "rimraf dist/*bundle* dist/*vendor* dist/*index*",
    "dev": "webpack-dev-server --devtool eval --progress --colors --content-base dist --hot --inline --display-error-details",
    "eslint": "eslint --ext .js,.jsx src",
    "stylelint": "stylelint \"src/**/*.css\"",
    "lesshint": "lesshint src/"
  }
}
